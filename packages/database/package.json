{
  "name": "@operate/database",
  "version": "0.1.0",
  "private": true,
  "description": "Operate Database - Prisma Client",
  "main": "index.js",
  "scripts": {
    "generate": "prisma generate",
    "migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "npx ts-node --esm prisma/seed.ts",
    "db:seed:test-company": "npx tsx prisma/seeds/test-company/index.ts",
    "studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.10.0",
    "prisma": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
